namespace cc;
class Program
{
    static void Main(string[] args)
    {
        int n = int.Parse(Console.ReadLine());
        int maxNum = int.MinValue;

        for(int i = 0; i < n; i++)
        {
            int current = int.Parse(Console.ReadLine());
            if(current > maxNum)
            {
                maxNum = current; 
            }
            
        }
        Console.WriteLine(maxNum);
    }
}


namespace dd;
class Program
{
    static void Main(string[] args)
    {
        int count = int.Parse(Console.ReadLine());

        int sum = 0;

        for(int i = 1; i <= count; i++)
        {
            string current = Console.ReadLine();
            int num = 0;

            if(current == "a")
            {
                num = 1; 
            }
            else if (current == "e")
            {
                num = 2;
            }
            else if (current == "i")
            {
                num = 3;
            }
            else if (current == "o")
            {
                num = 4;
            }
            else if (current == "u")
            {
                num = 5;
            }

            sum += num;
        }
        Console.WriteLine(sum);
    }
}

